ku_tables {
    templates {
        # cat=KU Tables/file; type=string; label=Layout Root Path: Path to templates (with a trailing slash).
        templateRootPath = EXT:ku_tables/Resources/Private/Templates/
        # cat=KU Tables/file; type=string; label=Partial Root Path: Path to partials (with a trailing slash).
        partialRootPath = EXT:ku_tables/Resources/Private/Partials/
        # cat=KU Tables/file; type=string; label=Layout Root Path: Path to templates (with a trailing slash).
        layoutRootPath = EXT:ku_tables/Resources/Private/Layouts/
    }
    assets {
        # cat=KU tables/Assets/t0; type=string; label=CSS assets: Path to stylesheet.
        Css = EXT:ku_tables/Resources/Public/Css/Dist/datatables.min.css
        # cat=KU tables/Assets/t1; type=string; label=JavaScript assets: Path to javaScript file.
        Js = EXT:ku_tables/Resources/Public/JavaScript/Dist/datatables.min.js
        # cat=KU tables/Assets/t2; type=string; label=Datatables language file. Path without language and file extension, e.g.: //cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Danish.json, becomes: //cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/
        datatableLangFilePath = //cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/
    }
}